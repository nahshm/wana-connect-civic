# Supabase Configuration
# Get these values from https://app.supabase.com/project/_/settings/api

VITE_SUPABASE_PROJECT_ID=your_project_id_here
VITE_SUPABASE_PUBLISHABLE_KEY=your_publishable_key_here
VITE_SUPABASE_URL=https://your_project_id.supabase.co

# Note: The PUBLISHABLE_KEY (anon key) is safe to expose in client-side code
# It's protected by Supabase Row Level Security (RLS) policies
# Never commit the service_role key to version control

# OAuth Provider Configuration
# Set to 'true' to show OAuth buttons in auth modal
# Only enable providers you've configured in Supabase Dashboard > Authentication > Providers
VITE_SUPABASE_GOOGLE_ENABLED=false
VITE_SUPABASE_APPLE_ENABLED=false
VITE_SUPABASE_GITHUB_ENABLED=false
